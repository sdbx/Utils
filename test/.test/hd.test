add(tests, &tlen, (testcase) {
   .name = "no-heading",
   .argv = "",
   .input = "test",
   .output = "# test\n"
});

add(tests, &tlen, (testcase) {
   .name = "heading",
   .argv = "",
   .input = "# test",
   .output = "# test\n"
});

add(tests, &tlen, (testcase) {
   .name = "no_heading_multiline",
   .argv = "",
   .input = "test\ntest2\n",
   .output = "# test\n# test2\n\n"
});

add(tests, &tlen, (testcase) {
   .name = "heading_multiline",
   .argv = "",
   .input = "# test\n# test2\n",
   .output = "# test\n# test2\n\n"
});